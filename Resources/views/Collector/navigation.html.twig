{% extends "WebProfilerBundle:Profiler:layout.html.twig" %}

{% block toolbar %}
    {% set icon %}
        <img alt="STEP NAVIGATION" src="data:image/png;base64," />
        <span class="sf-toolbar-status">
            <span class="sf-toolbar-info-class sf-toolbar-info-with-next-pointer">
                <abbr title="{{ collector.navigation.map.name }}">
                    map
                    {#{ collector.navigation.map.shortName }#}
                 </abbr>
            </span>
            {{ collector.navigation.currentStep.name }}
        </span>
    {% endset %}
    {% set text %}
        <div class="sf-toolbar-info-piece">
            <b>Map</b>
            <span>{{ collector.navigation.map.name }}</span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>Data store</b>
            <span>{{ collector.navigation.map.configuration.options.data_store }}</span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>Current step</b>
            <span>{{ collector.navigation.currentStep.name }}</span>
        </div>
    {% endset %}
    {% include "@WebProfiler/Profiler/toolbar_item.html.twig" with { "link": profiler_url } %}
{% endblock %}

{% block menu %}
    <span class="label">
        <span class="icon">
            <img alt="STEP NAVIGATION" src="data:image/png;base64," />
        </span>
        <strong>STEP NAVIGATION</strong>
        <span class="todo">
            <span>{# collector. #}</span>
            <span>{# #}</span>
        </span>
    </span>
{% endblock %}

{% block panel %}
    <h2>Step Navigation</h2>
{% endblock %}
